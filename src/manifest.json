{
  "manifest_version": 2,
  "name": "漆黒の射手リタさんがチラチラ見てくる",
  "author": "zenith",
  "short_name": "aigisrita",
  "description": "千年戦争アイギスの漆黒の射手リタさんがチラチラ見てきます。99%邪魔用です。",
  "version": "1.4.0",
  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },
  "permissions": [
    "background",
    "storage",
    "tabs",
    "idle",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "page": "background/background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/icon_19.png"
    },
    "default_popup": "browser_action/popup.html"
  },
  "web_accessible_resources": [
    "assets/*.png",
    "assets/*.json"
  ]
}
